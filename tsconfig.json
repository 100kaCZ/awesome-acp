{
	"compilerOptions": {
		"jsx": "react",
		"target": "es5",
		"module": "commonjs",
		"lib": ["dom", "es2015"],
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"esModuleInterop": true
	}
}
